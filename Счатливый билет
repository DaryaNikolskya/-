<html>
 <head> <meta charset="utf-8">
 <title>Счастливый билет</title>

 <script>
 function newCh(){
 for (var n = prompt ('введите номер билета', ''),
 a = n.length / 2,
 w = n.split (''), s = j = 0; j < a; j++)
 s += parseInt (w [j]) - w [j + a];
 alert ((!s ? '' : 'не') + 'счастливый');
 }
 </script>
</head>
<body>
<input type="button" value="Напишите число" onClick="newCh()">
<div id="res"></div>
</body>
</html>
